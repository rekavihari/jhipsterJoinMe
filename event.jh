entity Participant {
	name String,
    email String,
    phone Integer,
    image ImageBlob
}

entity Event {
	id String,
    name String,
    date String,
    desc String,
    image ImageBlob,
    startDate Instant,
	endDate Instant
}

entity Program {
    name String,
    desc String,
    image ImageBlob,
    latitude Double,
    longitude Double,
    startDate Instant,
	endDate Instant
}

relationship OneToMany {
	Event{program(name)} to Program{event(name)}
}

relationship ManyToMany {
	Event{participant(email)} to Participant{event(name)}
}

// Set pagination options
paginate Program, Event, Participant with infinite-scroll

